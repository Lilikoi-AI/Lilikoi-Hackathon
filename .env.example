# Required for wallet connection
# Get this from https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Required for chat functionality
# Get this from https://platform.openai.com/api-keys
# Use either OPENAI_API_KEY (more secure, server-side only)
# or NEXT_PUBLIC_OPENAI_API_KEY (less secure, available on client-side)
OPENAI_API_KEY=
# NEXT_PUBLIC_OPENAI_API_KEY=

# Required for staking functionality
# Default values are provided in the code, but you can override them here
NEXT_PUBLIC_SFC_CONTRACT_ADDRESS=0xFC00FACE00000000000000000000000000000000
NEXT_PUBLIC_S_TOKEN_ADDRESS=0x8c8687fC965593DFb2F0b4EAeFD55E9D8df348df

# Optional: Set this in production to your deployed URL
NEXT_PUBLIC_SITE_URL=http://localhost:3000
